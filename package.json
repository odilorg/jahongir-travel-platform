{
  "name": "jahongir-travel-platform",
  "version": "1.0.0",
  "private": true,
  "description": "Modern travel platform for Uzbekistan tours - Customer website, Admin panel, and API",
  "scripts": {
    "dev": "turbo dev",
    "dev:safe": "./scripts/check-health.sh && turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "clean:cache": "./scripts/clean-cache.sh",
    "clean:deep": "./scripts/clean-cache.sh --deep",
    "health": "./scripts/check-health.sh",
    "type-check": "turbo type-check"
  },
  "devDependencies": {
    "@turbo/gen": "^2.3.3",
    "maildev": "^2.2.1",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^18.3.27",
      "@types/react-dom": "^18.3.5"
    }
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
